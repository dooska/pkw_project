<%= form_for(@constituency) do |f| %>
  <% if @constituency.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@constituency.errors.count, "error") %> prohibited this constituency from being saved:</h2>

      <ul>
      <% @constituency.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class ="form-group">
    <%= f.label :Województwo %>
    <%= f.select :voivodeship_id,
        Voivodeship.all.collect { |x| [x.name, x.id]},
        {:include_blank => 'Wybierz województwo'},
        class:'form-control' %>
  </div>
  <div class="field">
    <%= f.label :voters %><br>
    <%= f.number_field :voters %>
  </div>
  <div class="field">
    <%= f.label :used_ballots %><br>
    <%= f.number_field :used_ballots %>
  </div>
  <div class="field">
    <%= f.label :invalid_no_chose %><br>
    <%= f.number_field :invalid_no_chose %>
  </div>
  <div class="field">
    <%= f.label :invalid_more_choises %><br>
    <%= f.number_field :invalid_more_choises %>
  </div>
  <div class="field">
    <%= f.label :invalid_other %><br>
    <%= f.number_field :invalid_other %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>